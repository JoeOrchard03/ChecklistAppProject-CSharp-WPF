<Window x:Class="InvTrackerApp.CustomListWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Check list" Height="175" Width="300">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <!-- <Button Grid.Row="0" Margin = "0,10,0,0" Content="Back" Width="Auto" Click="GoBack_Click" VerticalAlignment="Center"/> -->
        <TextBox Grid.Row="1" Margin = "0,0,0,0" Text="Input Custom List Name" Width="Auto" VerticalAlignment="Center"/>
        
    </Grid>
    
   
    
</Window>
 